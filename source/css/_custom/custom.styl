// Custom styles.
h4
  padding: 7px 6px 7px 7px;
  background: #dddddd;
  border-left: 5px solid $grey-light;
h5
  border-bottom: 1px #d8d8d8 solid;
.site-meta
  margin-left: 0px;
ruby
  display:inline-table;
  text-align:center;
  white-space:nowrap;
  text-indent:0;
  margin:0;
  vertical-align:text-bottom;
  line-height:1;
rb
  display:table-row-group;
rt
  font-size:60%;
  margin:0;
  line-height:100%;
  display:table-header-group;
  letter-spacing:0;
  text-align:center;
  color:#5DAC81;
.emoji
  display: inline;
  width: 30px;
  padding: 0!important;
  border: 0!important;
  vertical-align: middle;