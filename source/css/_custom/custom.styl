// Custom styles.
h4
  padding: 7px 6px 7px 0px;
  background: #dddddd;
  border-left: 5px solid $grey-light;
h5
  border-bottom: 1px #d8d8d8 solid;
.post
  code
    border:1px dashed #CCCCFF
    color: $red
.site-meta
  margin-left: 0px;

 /* change result's font color */
.tinysou-widget .autocomplete ul li p.title {
  color: $black-deep;
}
/* change highlighted result's background color */
.tinysou-widget .autocomplete ul li.active {
  background: none;
  background-color: $grey-dark;
  border-top: 1px solid transparent;
  border-bottom: 1px solid transparent;
  -webkit-box-shadow: 0 1px 0 $grey-dark inset;
}
/* change section's font color */
.tinysou-widget .autocomplete ul li.active .sections em {
  color: white;
}